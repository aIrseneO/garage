server {
	listen 80 default_server;
	listen [::]:80 default_server;

	server_name localhost;
	
	root /var/www/localhost;
	index index.php index.html;

	location / {
			try_files $uri $uri/ =404;
	}

	location ~ \.php$ {
		include snippets/fastcgi-php.conf;
		fastcgi_pass unix:/var/run/php/php7.3-fpm.sock;
	}
}

